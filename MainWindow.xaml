<Window x:Class="html_exctractor.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:html_exctractor"
        xmlns:md="http://materialdesigninxaml.net/winfx/xaml/themes"
        mc:Ignorable="d"
        MinHeight="500"
        MinWidth="1000">

    <md:DialogHost Identifier="dialog_host">
        <Grid>
            <Frame NavigationUIVisibility="Hidden"
                   x:Name="main_frame" />
            <md:Snackbar x:Name="snack_bar"
                         Background="{DynamicResource chip_background_color}"
                         Foreground="{DynamicResource text_light_color}"
                         FontFamily="{StaticResource MaterialDesignFont}" />
        </Grid>
    </md:DialogHost>


</Window>
